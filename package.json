{
    "name": "vite-envs",
    "version": "4.5.0",
    "description": "Env var in Vite at container startup",
    "repository": {
        "type": "git",
        "url": "git://github.com/garronej/vite-envs.git"
    },
    "scripts": {
        "build": "tsx scripts/build.ts",
        "link-in-starter": "tsx scripts/link-in-starter.ts",
        "link-in-app": "tsx scripts/link-in-app.ts",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different"
    },
    "bin": {
        "vite-envs": "dist/bin/main.js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./client": "./client.d.ts",
        "./types/hot": "./types/hot.d.ts",
        "./types/importGlob": "./types/importGlob.d.ts"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "files": [
        "src/",
        "dist/index.js",
        "dist/index.d.ts",
        "dist/vite-plugin.d.ts",
        "dist/bin/main.js"
    ],
    "keywords": [
        "react",
        "vite",
        "environment-variables",
        "typescript"
    ],
    "homepage": "https://github.com/garronej/vite-envs",
    "dependencies": {},
    "devDependencies": {
        "@types/ejs": "^3.1.4",
        "@types/node": "^20.8.10",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^2.8.8",
        "tsx": "^4.17.0",
        "typescript": "^5.5.4",
        "vite": "^5.1.3",
        "json5": "^2.2.3",
        "yaml": "^2.3.4",
        "cheerio": "^1.0.0",
        "dotenv": "^16.4.4",
        "ejs": "^3.1.9",
        "magic-string": "^0.30.7",
        "tsafe": "^1.8.5",
        "chokidar": "^3.6.0",
        "powerhooks": "^1.0.19",
        "@vercel/ncc": "^0.38.1",
        "evt": "^2.5.8",
        "run-exclusive": "^2.2.19"
    }
}
